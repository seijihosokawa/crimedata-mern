{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "index.js",
            "method": ["GET"]
        },
        {
            "src":"/summary",
            "dest": "routes/summary.js",
            "method": ["GET"]
        }
    ]
}